FoamFile
{
    version     2.2;
    format      ascii;
    class       dictionary;
    note        "mesh decomposition control dictionary";
    object      decomposeParDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

numberOfSubdomains  12;

method          simple;

simpleCoeffs
{
    n           (1 12 1);
    delta       0.001;
}

